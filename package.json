{
  "name": "peddler",
  "version": "1.0.0",
  "description": "Cloud-based alerting system for monitoring Facebook Marketplace and other marketplaces",
  "main": "index.js",
  "bin": {
    "peddler": "./bin/peddler"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f scheduler",
    "invoke": "serverless invoke -f scheduler"
  },
  "keywords": [
    "marketplace",
    "scraper",
    "alerts",
    "facebook-marketplace",
    "serverless",
    "aws"
  ],
  "author": "Kevin Marx",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "serverless": "^3.34.0",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.395.0",
    "@aws-sdk/client-eventbridge": "^3.395.0",
    "@aws-sdk/client-secrets-manager": "^3.395.0",
    "@aws-sdk/client-ssm": "^3.395.0",
    "@aws-sdk/lib-dynamodb": "^3.395.0",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "playwright": "^1.37.0",
    "uuid": "^9.0.0"
  }
}
